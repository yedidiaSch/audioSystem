<?xml version="1.0" encoding="UTF-8"?>
<audioSystemConfig>
    <audio>
        <sampleRate>44100.0</sampleRate>
        <bufferFrames>512</bufferFrames>
    </audio>
    
    <waveform>
        <type>sine</type>
        <!-- Available types: sine, square, sawtooth, triangle -->
    </waveform>
    
    <effects>
        <!-- Effects are applied in order -->
        <effect>delay</effect>
        <effect>lowpass</effect>
        <!-- Available effects: octave, delay, echo, lowpass, lpf, filter -->
    </effects>
    
    <midi>
        <port>1</port>
    </midi>
    
    <!-- Default frequency for testing (Hz) -->
    <defaultFrequency>440.0</defaultFrequency>
</audioSystemConfig>