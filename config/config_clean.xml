<?xml version="1.0" encoding="UTF-8"?>
<!-- Example configuration for pure, clean sound -->
<audioSystemConfig>
    <audio>
        <sampleRate>44100.0</sampleRate>
        <bufferFrames>512</bufferFrames>
    </audio>
    
    <waveform>
        <type>sine</type>
    </waveform>
    
    <effects>
        <!-- No effects for pure tone -->
    </effects>
    
    <midi>
        <port>1</port>
    </midi>
    
    <defaultFrequency>440.0</defaultFrequency>
</audioSystemConfig>